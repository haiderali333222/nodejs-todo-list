{
    "env": {
        "browser": true,
        "es2021": true,
        "amd": true,
        "commonjs": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-unresolved": 0,
        "no-await-in-loop": 0,
        "no-console": 0,
        "camelcase": 0,
        "no-unused-vars": 0,
        "default-param-last": 0,
        "import/no-cycle": 0,
        "consistent-return": 0,
        "import/no-named-as-default": 0,
        "no-tabs": 0,
        "no-return-assign": 0,
        "no-param-reassign": 0,
        "no-plusplus": 0,
        "no-shadow": 0,
        "no-use-before-define": 0,
        "no-useless-concat": 0,
        "new-cap": 0,
        "prefer-promise-reject-errors": 0,
        "no-promise-executor-return": 0,
        "no-async-promise-executor": 0,
        "linebreak-style": 0
    }
}